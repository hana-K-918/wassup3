coffee = 10
money = 300

while True :
    money = int(input("돈을 주세요"))
    if money == 300:
        print('돈을 받았으니 커피를 줍니다.')
        coffee = coffee -1
    elif money > 300:
        print(f"거스름돈 {money -300}를 주고 커피를 줍니다.")
        coffee = coffee - 1
    elif money < 300:
        print('돈을 더 주세요')
        coffee = coffee
    print(f'남은 커피의 양은 {coffee}입니다.')
    if coffee == 0:
        print('커피가 다 떨어져서 판매를 중지합니다.')
        break





coffee = 5

while True:
    money = int(input('돈을 넣어주세요'))

    if money == 300:
        print('300원을 받았습니다. 커피를 줍니다.')
        coffee = coffee -1
        print(f'남은 커피수량은 {coffee}입니다')
    elif money > 300:
        print(f'거스름돈 {money-300}원을 주고 커피를 줍니다.')
        coffee = coffee -1
        print(f'남은 커피수량은 {coffee}입니다')
    else: # money<300 
        print('돈이 부족합니다.')

    if coffee == 0:
        print('커피가 다 떨어졌습니다')
        break








